<Window x:Class="WPFAnimalSounds.Window4"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:WPFAnimalSounds"
        mc:Ignorable="d"
        Title="Window4" Height="450" Width="800">
    <Grid>
        <Image x:Name="img1" HorizontalAlignment="Left" VerticalAlignment="Top" Height="75"
            Width="150" Margin="10,20,0,0" Source="FerrariF40.png"/>
        <Image x:Name="img2" HorizontalAlignment="Left" VerticalAlignment="Top" Height="75"
            Width="150" Margin="10,125,0,0" Source="FerrariF40.png"/>
        <Image x:Name="img3" HorizontalAlignment="Left" VerticalAlignment="Top" Height="75"
            Width="150" Margin="10,230,0,0" Source="FerrariF40.png"/>
        <Image x:Name="img4" HorizontalAlignment="Left" VerticalAlignment="Top" Height="75"
            Width="150" Margin="10,335,0,0" Source="FerrariF40.png"/>
        <Button x:Name="btnStart" Content="Start" Width="75" HorizontalAlignment="Left"
            VerticalAlignment="Bottom" Margin="255,211,0,0" Click="BtnStart_Click"/>
    </Grid>
</Window>
